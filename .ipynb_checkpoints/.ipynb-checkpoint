   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Median height of goalkeepers: 178.9106330846165\n",
      "Median height of midfielders: 181.87731009161627\n",
      "Median height of attackers: 179.9995052737052\n",
      "Median height of goal keepers and attackers combined: 179.03770191505464\n"
     ]
    }
   ],
   "source": [
    "gk_heights = np_heights[(np_positions == 'GK')]\n",
    "print(\"Median height of goalkeepers: \" + str(np.median(gk_heights)))\n",
    "m_heights = np_heights[np_positions == 'M']\n",
    "print(\"Median height of midfielders: \" + str(np.median(m_heights)))\n",
    "a_heights = np_heights[np_positions == 'A']\n",
    "print(\"Median height of attackers: \" + str(np.median(a_heights)))\n",
    "gk_a_heights = np.append(gk_heights, a_heights)\n",
    "print(\"Median height of goal keepers and attackers combined: \" + str(np.median(gk_a_heights)))\n"
   ]
  },
